__workspace()
{
    local modes
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"

    modes="apply list config"

    case "$prev" in
        "workspace" )
            COMPREPLY=( $(compgen -W "${modes}" -- ${cur}) )
            ;;
        "apply" )
            COMPREPLY=( $(compgen -W "$(workspace list)" -- ${cur}) )
            ;;
        # * )
        #     ;;
    esac
}

complete -F __workspace workspace
